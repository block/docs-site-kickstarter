---
import BlogSection from "@/components/block/BlogSection.astro"

const { posts } = Astro.props;
---

<section>

    <BlogSection
      title="Bob Loblaw Law Blog"
      description="That's a low blow, Loblaw. Bob Loblaw Law Bomb."
      posts={posts}
      showExcerpts={true}
      showCategories={true}
      showAuthors={true}
      showReadTime={true}
      layout="list"
    />
    
</section>